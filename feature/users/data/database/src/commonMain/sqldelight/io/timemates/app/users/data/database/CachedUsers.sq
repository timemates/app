CREATE TABLE CachedUsers (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    description TEXT NOT NULL,
    avatarFileId TEXT NOT NULL,
    emailAddress TEXT
);

insert:
INSERT INTO CachedUsers (id, name, description, avatarFileId, emailAddress)
VALUES (?, ?, ?, ?, ?);

delete:
DELETE FROM CachedUsers
WHERE id = ?;

get:
SELECT *
FROM CachedUsers
WHERE id IN (?);

clear:
DELETE FROM CachedUsers;


